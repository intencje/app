{
  "name": "Intencje.pl",
  "version": "1.0.0",
  "author": "Sławomir Jakubowski",
  "homepage": "https://intencje.pl",
  "scripts": {
    "compodoc": "npx compodoc -p tsconfig.json",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "eslint": "npx eslint -c .eslintrc.js --ext .ts src",
    "dev:ssr": "ng run intencje:serve-ssr",
    "serve:ssr": "node dist/intencje/server/main.js",
    "build:ssr": "ng build --prod && ng run intencje:server:production",
    "build:shell": "ng run intencje:app-shell:production",
    "prerender": "ng run intencje:prerender",
    "electron:devel": "ng build --prod --base-href=./ && npx cap copy && npx cap open electron",
    "electron:mac": "electron-packager ./electron Intencje --overwrite --platform=darwin --arch=x64 --prune=true --out=release-builds",
    "electron:win": "electron-packager ./electron Intencje --overwrite --asar=true --platform=win32 --arch=ia32 --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName='Platforma Duchowej Łączności'",
    "electron:linux": "electron-packager ./electron Intencje --overwrite --asar=true --platform=linux --arch=x64 --prune=true --out=release-builds",
    "cypress": "$(npm bin)/cypress run",
    "cypress:open": "$(npm bin)/cypress open"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.0.3",
    "@angular/cdk": "^11.0.1",
    "@angular/common": "~11.0.3",
    "@angular/compiler": "~11.0.3",
    "@angular/core": "~11.0.3",
    "@angular/fire": "^6.1.3",
    "@angular/flex-layout": "^11.0.0-beta.33",
    "@angular/forms": "~11.0.3",
    "@angular/material": "^11.0.1",
    "@angular/platform-browser": "~11.0.3",
    "@angular/platform-browser-dynamic": "~11.0.3",
    "@angular/platform-server": "~11.0.3",
    "@angular/router": "~11.0.3",
    "@angular/service-worker": "~11.0.3",
    "@capacitor/android": "^2.4.4",
    "@capacitor/core": "2.4.4",
    "@capacitor/ios": "^2.4.4",
    "@ckeditor/ckeditor5-angular": "^2.0.1",
    "@ckeditor/ckeditor5-build-balloon": "^23.1.0",
    "@ckeditor/ckeditor5-build-classic": "^23.1.0",
    "@finosofica/mat-contenteditable": "^9.1.2",
    "@ionic/pwa-elements": "^3.0.1",
    "@nguniversal/express-engine": "^11.0.0",
    "@ngx-loading-bar/core": "^5.1.0",
    "@ngx-loading-bar/http-client": "^5.1.1",
    "@ngx-loading-bar/router": "^5.1.0",
    "@sentry/angular": "^5.28.0",
    "@sentry/tracing": "^5.28.0",
    "@tinymce/tinymce-angular": "^4.2.0",
    "Intencje": "file:electron",
    "express": "^4.17.1",
    "firebase": "^8.1.1",
    "hammerjs": "^2.0.8",
    "howler": "^2.2.1",
    "lottie-web": "^5.7.4",
    "mat-contenteditable": "^9.1.0",
    "mat-ngx-wig": "^7.1.0",
    "ngx-electron": "^2.2.0",
    "ngx-image-cropper": "^3.3.1",
    "ngx-infinite-scroll": "^10.0.0",
    "ngx-input-search": "^3.0.8",
    "ngx-lottie": "^6.4.0",
    "ngx-mat-select-search": "^3.1.4",
    "ngx-skeleton-loader": "^2.6.1",
