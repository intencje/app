{
  "name": "Intencje",
  "author": {
    "name": "Sławomir Jakubowski",
    "email": "jakubowski@slawomir.info"
  },
  "homepage": "https://intencje.pl",
  "description": "Platforma Duchowej Łączności",
  "license": "MIT",
  "version": "1.0.14",
  "scripts": {
    "postinstall": "husky install && electron-builder install-app-deps",
    "compodoc": "npx compodoc -p tsconfig.json",
    "ng": "ng",
    "ng:serve": "ng serve",
    "start": "npm-run-all -p electron:serve ng:serve",
    "build": "npm run electron:serve-tsc && ng build --configuration=standalone --base-href ./",
    "build:capacitor:ios": "ng build --configuration=standalone && npx cap sync ios",
    "build:capacitor:android": "ng build --configuration=standalone && npx cap sync android",
    "test": "ng test",
    "lint": "ng lint",
    "eslint": "npx eslint -c .eslintrc.js --ext .ts src",
    "dev:ssr": "ng run intencje:serve-ssr",
    "serve:ssr": "node dist/intencje/server/main.js",
    "build:ssr": "ng build --prod && ng run intencje:server:production",
    "build:shell": "ng run intencje:app-shell:production",
    "prerender": "ng run intencje:prerender",
    "cypress": "$(npm bin)/cypress run",
    "cypress:open": "$(npm bin)/cypress open",
    "electron:serve-tsc": "tsc -p tsconfig.serve.json",
    "electron:serve": "wait-on tcp:4200 && npm run electron:serve-tsc && npx electron . --serve",
    "electron:local": "npm run build && npx electron .",
    "electron:build": "npm run build && electron-builder build --publish=never"
  },
  "main": "main.js",
  "build": {
    "appId": "pl.intencje",
    "productName": "Intencje",
    "files": [
      "**/*",
      "!**/*.ts",
      "!*.code-workspace",
      "!LICENSE.md",
      "!package.json",
      "!package-lock.json",
      "!android/",
      "!cypress/",
      "!docs/",
      "!functions/",
      "!ios/",
      "!src/",
      "!node_modules/**/*"
    ],
    "mac": {
      "icon": "dist/browser/assets/icons",
      "target": [
        "dmg"
      ],
      "category": "public.app-category.social-networking"
    },
    "linux": {
      "icon": "dist/browser/assets/icons",
      "target": [
        "AppImage"
      ],
      "category": "Utility"
    },
    "win": {
      "icon": "dist/browser/assets/icons",
      "publisherName": "Sławomir Jakubowski",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "ia32",
            "x64"
          ]
        }
      ]
    },
    "publish": {
      "provider": "github",
      "releaseType": "release"
    },
    "directories": {
      "output": "release/"
    }
  },
  "dependencies": {
    "@angular/animations": "~11.1.0",
    "@angular/cdk": "^11.1.0",
    "@angular/common": "~11.1.0",
    "@angular/compiler": "~11.1.0",
    "@angular/core": "~11.1.0",
    "@angular/fire": "^6.1.4",
    "@angular/flex-layout": "^11.0.0-beta.33",
    "@angular/forms": "~11.1.0",
    "@angular/material": "^11.1.0",
    "@angular/platform-browser": "~11.1.0",
    "@angular/platform-browser-dynamic": "~11.1.0",
    "@angular/platform-server": "~11.1.0",
    "@angular/router": "~11.1.0",
    "@angular/service-worker": "~11.1.0",
    "@capacitor/android": "^2.4.6",
    "@capacitor/angular": "^1.0.3",
    "@capacitor/core": "^2.4.6",
    "@capacitor/ios": "^2.4.6",
    "@ckeditor/ckeditor5-angular": "^2.0.1",
    "@ckeditor/ckeditor5-build-balloon": "^24.0.0",
    "@ckeditor/ckeditor5-build-classic": "^24.0.0",
    "@finosofica/mat-contenteditable": "^9.1.2",
    "@ionic/pwa-elements": "^3.0.2",
    "@nguniversal/express-engine": "^11.1.0",
    "@ngx-loading-bar/core": "^5.1.1",
    "@ngx-loading-bar/http-client": "^5.1.1",
    "@ngx-loading-bar/router": "^5.1.1",
    "@sentry/angular": "^6.0.1",
    "@sentry/tracing": "^6.0.1",
    "@tinymce/tinymce-angular": "^4.2.0",
    "cypress": "^6.5.0",
    "express": "^4.17.1",
    "firebase": "^8.2.4",
    "hammerjs": "^2.0.8",
    "howler": "^2.2.1",
    "lottie-web": "^5.7.6",
    "mat-contenteditable": "^9.1.0",
    "mat-ngx-wig": "^7.1.0",
    "ngx-electron": "^2.2.0",
    "ngx-image-cropper": "^3.3.3",
    "ngx-infinite-scroll": "^10.0.1",
    "ngx-lottie": "^6.4.0",
    "ngx-mat-select-search": "^3.2.0",
    "ngx-skeleton-loader": "^2.6.2",
    "rxjs": "~6.6.3",
    "tinymce": "^5.6.2",
    "tslib": "^2.1.0",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1101.1",
    "@angular-devkit/schematics": "^11.2.0",
    "@angular/cli": "~11.1.1",
    "@angular/compiler-cli": "~11.1.0",
    "@angular/language-service": "~11.1.0",
    "@capacitor/cli": "^2.4.6",
    "@compodoc/compodoc": "^1.1.11",
    "@nguniversal/builders": "^11.1.0",
    "@types/express": "^4.17.11",
    "@types/hammerjs": "^2.0.36",
    "@types/howler": "^2.2.1",
    "@types/node": "^14.14.22",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "codelyzer": "^6.0.1",
    "electron": "11.2.0",
    "electron-builder": "22.9.1",
    "electron-packager": "^15.2.0",
    "electron-reload": "1.5.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "firebase-tools": "^9.2.2",
    "husky": "^5.1.1",
    "prettier": "^2.2.1",
    "ts-node": "~9.1.1",
    "tslint": "~6.1.3",
    "typescript": "~4.1.3",
    "wait-on": "^5.2.1"
  }
}
