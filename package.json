{
  "name": "Intencje.pl",
  "version": "1.0.0",
  "author": "Sławomir Jakubowski",
  "homepage": "https://intencje.pl",
  "scripts": {
    "compodoc": "npx compodoc -p tsconfig.json",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "eslint": "npx eslint -c .eslintrc.js --ext .ts src",
    "dev:ssr": "ng run intencje:serve-ssr",
    "serve:ssr": "node dist/intencje/server/main.js",
    "build:ssr": "ng build --prod && ng run intencje:server:production",
    "build:shell": "ng run intencje:app-shell:production",
    "prerender": "ng run intencje:prerender",
    "electron:devel": "ng build --prod --base-href=./ && npx cap copy && npx cap open electron",
    "electron:mac": "electron-packager ./electron Intencje --overwrite --platform=darwin --arch=x64 --prune=true --out=release-builds",
    "electron:win": "electron-packager ./electron Intencje --overwrite --asar=true --platform=win32 --arch=ia32 --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName='Platforma Duchowej Łączności'",
    "electron:linux": "electron-packager ./electron Intencje --overwrite --asar=true --platform=linux --arch=x64 --prune=true --out=release-builds"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.0.4",
    "@angular/cdk": "^10.0.2",
    "@angular/common": "~10.0.4",
    "@angular/compiler": "~10.0.4",
    "@angular/core": "~10.0.4",
    "@angular/fire": "^6.0.2",
    "@angular/flex-layout": "^10.0.0-beta.32",
    "@angular/forms": "~10.0.4",
    "@angular/material": "^10.0.2",
    "@angular/platform-browser": "~10.0.4",
    "@angular/platform-browser-dynamic": "~10.0.4",
    "@angular/platform-server": "~10.0.4",
    "@angular/router": "~10.0.4",
    "@angular/service-worker": "~10.0.4",
    "@capacitor/android": "^2.3.0",
    "@capacitor/core": "2.3.0",
    "@capacitor/ios": "^2.3.0",
    "@ckeditor/ckeditor5-angular": "^1.2.3",
    "@ckeditor/ckeditor5-build-classic": "^20.0.0",
    "@nguniversal/express-engine": "^10.0.1",
    "@ngx-loading-bar/core": "^5.1.0",
    "@ngx-loading-bar/router": "^5.1.0",
    "Intencje": "file:electron",
    "express": "^4.17.1",
    "firebase": "^7.16.1",
    "hammerjs": "^2.0.8",
    "howler": "^2.2.0",
    "lottie-web": "^5.7.0",
    "ngx-electron": "^2.2.0",
    "ngx-image-cropper": "^3.2.0",
    "ngx-infinite-scroll": "^9.0.0",
    "ngx-lottie": "^6.2.1",
    "ngx-skeleton-loader": "^2.2.1",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.3",
    "@angular-devkit/schematics": "^10.0.3",
    "@angular/cli": "~10.0.3",
    "@angular/compiler-cli": "~10.0.4",
    "@angular/language-service": "~10.0.4",
    "@capacitor/cli": "2.3.0",
    "@compodoc/compodoc": "^1.1.11",
    "@nguniversal/builders": "^10.0.1",
    "@types/express": "^4.17.7",
    "@types/howler": "^2.2.1",
    "@types/node": "^14.0.23",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "codelyzer": "^6.0.0",
    "electron-packager": "^15.0.0",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "firebase-tools": "^8.6.0",
    "npm-check-updates": "^7.0.2",
    "prettier": "^2.0.5",
    "ts-node": "~8.10.2",
    "tslint": "~6.1.2",
    "typescript": "~3.9.7"
  }
}
